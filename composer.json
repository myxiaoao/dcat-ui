{
    "name": "cooper/dcat-ui",
    "description": "Custom UI theme for Dcat Admin",
    "type": "library",
    "keywords": [
        "dcat-admin",
        "theme",
        "ui",
        "extension"
    ],
    "homepage": "https://github.com/myxiaoao/dcat-ui",
    "license": "MIT",
    "authors": [
        {
            "name": "Cooper",
            "email": "myxiaoao@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "ext-bcmath": "*",
        "ext-json": "*",
        "dcat/laravel-admin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Cooper\\DcatUi\\": "src/"
        }
    },
    "extra": {
        "dcat-admin": "Cooper\\DcatUi\\DcatUiServiceProvider",
        "laravel": {
            "providers": [
                "Cooper\\DcatUi\\DcatUiServiceProvider"
            ]
        }
    }
}
